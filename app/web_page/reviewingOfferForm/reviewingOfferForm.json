    },
    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.offer && $data.offer_government && $data.offer_government_governmentLevel ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.offer.government = $data.offer_government;",
        "\t$data.offer.government.governmentLevel = $data.offer_government_governmentLevel;",
        "\treturn {",
        "\t\t//map offer variable to expected task contract input",
        "\t\tofferInput: {",
        "\t\t\titems: $data.offer.items.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\t\tmonthlyCost: it.monthlyCost !== undefined ? it.monthlyCost : null,",
        "\t\t\t\tyearlyCost: it.yearlyCost !== undefined ? it.yearlyCost : null,",
        "\t\t\t\tname: it.name !== undefined ? it.name : null",
        "\t\t\t})),",
        "\t\t\tgovernment: $data.offer.government ? {",
        "\t\t\t\tpersistenceId_string: $data.offer.government.persistenceId_string !== undefined ? $data.offer.government.persistenceId_string : null",
        "\t\t\t} : null,",
        "\t\t\tisAccepted: $data.offer.isAccepted !== undefined ? $data.offer.isAccepted : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "offer_government_governmentLevel" : {
      "type" : "url",
      "value" : [
        "{{offer_government|lazyRef:'governmentLevel'}}"
      ],
