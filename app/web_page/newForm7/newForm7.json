    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.law && $data.law_questions ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.law.questions = $data.law_questions;",
        "\treturn {",
        "\t\t//map law variable to expected task contract input",
        "\t\tlawInput: {",
        "\t\t\tquestions: $data.law.questions.map( it => ({",
        "\t\t\t\tpersistenceId_string: it.persistenceId_string !== undefined ? it.persistenceId_string : null,",
        "\t\t\t\ttext: it.text !== undefined ? it.text : null",
        "\t\t\t}))",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}/context"
      ],
      "exposed" : false
    },
    "law_questions" : {
      "type" : "url",
      "value" : [
        "{{law|lazyRef:'questions'}}"
      ],
      "exposed" : false
    },
    "taskId" : {
